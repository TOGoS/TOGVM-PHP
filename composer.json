{
	"name": "togos/togvm",
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "togos/togvm-spec",
				"version": "0.4.0",
				"source": {
					"url": "https://github.com/TOGoS/TOGVM-Spec.git",
					"type": "git",
					"reference": "origin/master"
				}
			}
		}
	],
	"require": {
		"php": ">=5.4",
		"earthit/php-common": "^1.0.0",
		"ryprop/nife": "^0.2"
	},
	"require-dev": {
		"phpunit/phpunit": "^3.7",
		"togos/togvm-spec": "1.0.0",
		"togos/php-n2r": "^0.0.5"
	},
	"autoload": {
		"psr-0": {
			"TOGoS_TOGVM_": "lib/",
			"TOGoS_TOGES_": "lib/"
		}
	},
	"autoload-dev": {
		"psr-0": {
			"TOGoS_TOGVM_": "test/",
			"TOGoS_TOGES_": "test/"
		}
	}
}
